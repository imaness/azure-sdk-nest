{
  "version": "0.4.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@imaness/azure-sdk-nest",
  "author": "Emmanuel Soriano",
  "repository": {
    "type": "git",
    "url": "https://github.com/imaness/azure-sdk-nest"
  },
  "module": "dist/azure-sdk-nest.esm.js",
  "size-limit": [
    {
      "path": "dist/azure-sdk-nest.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/azure-sdk-nest.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@nestjs/common": "^9.1.4",
    "@size-limit/preset-small-lib": "^8.1.0",
    "husky": "^8.0.1",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "@nestjs/common": "*"
  },
  "dependencies": {
  },
  "keywords": [
    "NestJS",
    "Nest",
    "module",
    "azure",
    "service bus",
    "blob Storage",
    "nestjs azure sdk",
    "nest azure sdk"
  ]
}
